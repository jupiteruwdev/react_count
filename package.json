{
  "name": "react-countup",
  "version": "1.0.1",
  "description": "A React component wrapper around CountUp.js",
  "author": "Glenn Reyes",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/glennreyes/react-countup.git"
  },
  "bugs": {
    "url": "https://github.com/glennreyes/react-countup/issues"
  },
  "homepage": "https://github.com/glennreyes/react-countup#readme",
  "main": "lib/index.js",
  "scripts": {
    "prebuild": "npm t && npm run clean",
    "build": "cross-env NODE_ENV=production babel --out-dir=lib --ignore=test src",
    "clean": "rimraf lib",
    "coveralls": "npm run test:coverage && cat ./coverage/lcov.info | coveralls",
    "prepublish": "npm run build",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "cross-env NODE_ENV=test mocha --opts mocha.opts",
    "test:watch": "npm t -- --watch",
    "test:coverage": "cross-env NODE_ENV=test istanbul cover _mocha --report lcovonly -- -R spec --opts mocha.opts"
  },
  "keywords": [
    "react",
    "react.js",
    "countup",
    "countup.js",
    "counter",
    "animation"
  ],
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0"
  },
  "dependencies": {
    "countup.js": "^1.7.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "coveralls": "^2.11.9",
    "cross-env": "^1.0.8",
    "enzyme": "^2.3.0",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1",
    "expect": "^1.20.1",
    "express": "^4.13.4",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.5.3",
    "mocha-lcov-reporter": "^1.2.0",
    "react-addons-test-utils": "^15.1.0",
    "rimraf": "^2.5.2"
  }
}
